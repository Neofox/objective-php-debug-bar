{
  "name": "neofox/objective-php-debug-bar",
  "description": "PHPDebugBar integration for Objective PHP",
  "type": "library",
  "keywords": [
    "debug",
    "objective php"
  ],
  "homepage": "https://github.com/Neofox/objecive-php-debug-bar",
  "license": "MIT",
  "authors": [
    {
      "name": "Jerome Schaeffer",
      "email": "jer.schaeffer@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "objective-php/components": "1.0.*",
    "objective-php/message": "0.9.*",
    "objective-php/application": "1.0.*",
    "maximebf/debugbar": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "phpro/grumphp": "^0.10.2",
    "friendsofphp/php-cs-fixer": "^2.0",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "squizlabs/php_codesniffer": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "ObjectivePHP\\Package\\DebugBar\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c tests/phpunit.xml",
    "php-cs-fix": "vendor/bin/php-cs-fixer fix src/ && vendor/bin/php-cs-fixer fix tests/"
  }
}
